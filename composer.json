{
  "name": "i-lateral/silverstripe-custommenus",
  "description": "A Silverstripe module allowing you to create custom menus that can be gernated in the front end of your site.",
	"type": "silverstripe-vendormodule",
	"homepage": "http://github.com/i-lateral/silverstripe-custommenus",
	"keywords": [
		"silverstripe",
		"nav",
		"menus",
		"navigation"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Morven Lewis-Everley",
			"email": "morven@i-lateral.com"
		}
	],
	"support": {
		"issues": "http://github.com/i-lateral/silverstripe-custommenus/issues"
	},
	"require": {
		"silverstripe/framework": "^4.0",
        "silverstripe/cms": "^4.0",
		"silverstripe/siteconfig": "^4.0",
		"symbiote/silverstripe-gridfieldextensions": "^3.1",
		"nathancox/hasoneautocompletefield": "^2.0"
	},
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "silverstripe/vendor-plugin": "^1"
	},
    "autoload": {
        "psr-4": {
            "ilateral\\SilverStripe\\CustomMenus\\": "src/",
            "ilateral\\SilverStripe\\CustomMenus\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phpcs -s src/ tests/",
        "lint-clean": "phpcbf src/ tests/"
	}
}